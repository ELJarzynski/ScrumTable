{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      showMobileMenu: false,\n      cart: {\n        items: []\n      },\n      user: null\n    };\n  },\n  beforeCreate() {\n    this.$store.commit('initializeStore');\n    const token = this.$store.state.token;\n    if (token) {\n      axios.defaults.headers.common['Authorization'] = \"Token \" + token;\n      this.fetchUserData();\n    } else {\n      axios.defaults.headers.common['Authorization'] = \"\";\n    }\n  },\n  mounted() {\n    this.cart = this.$store.state.cart;\n  },\n  computed: {\n    cartTotalLength() {\n      let totalLength = 0;\n      for (let i = 0; i < this.cart.items.length; i++) {\n        totalLength += this.cart.items[i].quantity;\n      }\n      return totalLength;\n    }\n  },\n  methods: {\n    logout() {\n      // Usunięcie tokenu z localStorage\n      localStorage.removeItem('token');\n      // Ustawienie stanu autentykacji na false, za pomocą Vuex lub innego mechanizmu\n      // Przekierowanie użytkownika do strony logowania\n      this.$router.push('/log-in');\n    },\n    async fetchUserData() {\n      try {\n        const response = await axios.get('http://127.0.0.1:8000/user/User/');\n        this.user = response.data;\n      } catch (error) {\n        console.error('Error fetching user data:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","showMobileMenu","cart","items","user","beforeCreate","$store","commit","token","state","defaults","headers","common","fetchUserData","mounted","computed","cartTotalLength","totalLength","i","length","quantity","methods","logout","localStorage","removeItem","$router","push","response","get","error","console"],"sources":["C:\\Users\\kamil\\Desktop\\Studia\\Semestr VI\\ProjektZespołowy\\ScrumTable-visi-DziarskiQuartet\\frontend\\src\\views\\UserPanel.vue"],"sourcesContent":["<template>\r\n  <div id=\"app\">\r\n    <nav class=\"navbar is-dark\">\r\n      <div class=\"navbar-brand\">\r\n        <router-link to=\"/\" class=\"navbar-item\"><strong>Dziarski scrum</strong></router-link>\r\n      </div>\r\n\r\n       <div class=\"navbar-end\">\r\n          <div class=\"navbar-item\">\r\n            <div class=\"buttons\">\r\n              <template v-if=\"$store.state.isAuthenticated\">\r\n                <router-link to=\"/user-main\" class=\"button is-light\">Panel użytkownika</router-link>\r\n                <button @click=\"logout\" class=\"button is-light\">Wyloguj</button>\r\n                <span v-if=\"user\">{{ user.first_name }} {{ user.last_name }}</span>\r\n              </template>\r\n\r\n              <template v-else>\r\n                <router-link to=\"/log-in\" class=\"button is-light\">Zaloguj</router-link>\r\n              </template>\r\n          </div>\r\n          </div>\r\n        </div> \r\n    </nav>\r\n\r\n    <section class=\"section\">\r\n      <router-view/>\r\n    </section>\r\n\r\n    <footer class=\"footer\">\r\n      <p class=\"has-text-centered\">Visimind</p>\r\n    </footer>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      showMobileMenu: false,\r\n      cart: {\r\n        items: []\r\n      },\r\n      user: null\r\n    }\r\n  },\r\n  beforeCreate() {\r\n    this.$store.commit('initializeStore')\r\n\r\n    const token = this.$store.state.token\r\n\r\n    if (token) {\r\n        axios.defaults.headers.common['Authorization'] = \"Token \" + token\r\n        this.fetchUserData()\r\n    } else {\r\n        axios.defaults.headers.common['Authorization'] = \"\"\r\n    }\r\n  },\r\n  mounted() {\r\n    this.cart = this.$store.state.cart\r\n  },\r\n  computed: {\r\n      cartTotalLength() {\r\n          let totalLength = 0\r\n\r\n          for (let i = 0; i < this.cart.items.length; i++) {\r\n              totalLength += this.cart.items[i].quantity\r\n          }\r\n\r\n          return totalLength\r\n      }\r\n  },\r\n  methods: {\r\n    logout() {\r\n      // Usunięcie tokenu z localStorage\r\n      localStorage.removeItem('token');\r\n      // Ustawienie stanu autentykacji na false, za pomocą Vuex lub innego mechanizmu\r\n      // Przekierowanie użytkownika do strony logowania\r\n      this.$router.push('/log-in');\r\n    },\r\n    async fetchUserData() {\r\n      try {\r\n        const response = await axios.get('http://127.0.0.1:8000/user/User/');\r\n        this.user = response.data;\r\n      } catch (error) {\r\n        console.error('Error fetching user data:', error);\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n@import '~bulma/css/bulma.css';\r\n\r\n.lds-dual-ring {\r\n  display: inline-block;\r\n  width: 80px;\r\n  height: 80px;\r\n}\r\n.lds-dual-ring:after {\r\n  content: \" \";\r\n  display: block;\r\n  width: 64px;\r\n  height: 64px;\r\n  margin: 8px;\r\n  border-radius: 50%;\r\n  border: 6px solid #ccc;\r\n  border-color: #ccc transparent #ccc transparent;\r\n  animation: lds-dual-ring 1.2s linear infinite;\r\n}\r\n@keyframes lds-dual-ring {\r\n  0% {\r\n    transform: rotate(0deg);\r\n  }\r\n  100% {\r\n    transform: rotate(360deg);\r\n  }\r\n}\r\n\r\n.is-loading-bar {\r\n  height: 0;\r\n  overflow: hidden;\r\n\r\n  -webkit-transition: all 0.3s;\r\n  transition: all 0.3s;\r\n\r\n  &.is-loading {\r\n    height: 80px;\r\n  }\r\n}\r\n</style> \r\n"],"mappings":";AAmCA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,KAAK;MACrBC,IAAI,EAAE;QACJC,KAAK,EAAE;MACT,CAAC;MACDC,IAAI,EAAE;IACR;EACF,CAAC;EACDC,YAAYA,CAAA,EAAG;IACb,IAAI,CAACC,MAAM,CAACC,MAAM,CAAC,iBAAiB;IAEpC,MAAMC,KAAI,GAAI,IAAI,CAACF,MAAM,CAACG,KAAK,CAACD,KAAI;IAEpC,IAAIA,KAAK,EAAE;MACPT,KAAK,CAACW,QAAQ,CAACC,OAAO,CAACC,MAAM,CAAC,eAAe,IAAI,QAAO,GAAIJ,KAAI;MAChE,IAAI,CAACK,aAAa,CAAC;IACvB,OAAO;MACHd,KAAK,CAACW,QAAQ,CAACC,OAAO,CAACC,MAAM,CAAC,eAAe,IAAI,EAAC;IACtD;EACF,CAAC;EACDE,OAAOA,CAAA,EAAG;IACR,IAAI,CAACZ,IAAG