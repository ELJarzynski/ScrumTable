ogin() {\r\n      try {\r\n        const response = await axios.post('http://127.0.0.1:8000/user/token/', this.loginForm);\r\n        \r\n        const token = response.data.token;\r\n        localStorage.setItem('token', token); // Zapisz token w localStorage\r\n        console.log('Login successful');\r\n        console.log(token);\r\n\r\n        this.showNotification = true;\r\n\r\n        setTimeout(() => {\r\n          this.$router.push('/user-main-page');\r\n        }, 2000);\r\n      } catch (error) {\r\n        console.error('Login error:', error.response.data);\r\n      }\r\n    },\r\n  }\r\n};\r\n</script>"],"mappings":";AAgDA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,UAAU,EAAE,EAAE;QACdC,SAAS,EAAE,EAAE;QACbC,QAAQ,EAAE;MACZ,CAAC;MACDC,SAAS,EAAE;QACTJ,KAAK,EAAE,EAAE;QACTG,QAAQ,EAAE;MACZ,CAAC;MACDE,eAAe,EAAE;QACfC,IAAI,EAAE,EAAE;QACRC,WAAW,EAAE,EAAE;QACfC,QAAQ,EAAE;MACZ;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,KAAKA,CAAA,EAAG;MACZ,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMd,KAAK,CAACe,IAAI,CAAC,mCAAmC,EAAE,IAAI,CAACR,SAAS,CAAC;QAEtF,MAAMS,KAAI,GAAIF,QAAQ,CAACb,IAAI,CAACe,KAAK;QACjCC,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEF,KAAK,CAAC,EAAE;QACtCG,OAAO,CAACC,GAAG,CAAC,kBAAkB,CAAC;QAC/BD,OAAO,CAACC,GAAG,CAACJ,KAAK,CAAC;QAElB,IAAI,CAACK,gBAAe,GAAI,IAAI;QAE5BC,UAAU,CAAC,MAAM;UACf,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,iBAAiB,CAAC;QACtC,CAAC,EAAE,IAAI,CAAC;MACV,EAAE,OAAOC,KAAK,EAAE;QACdN,OAAO,CAACM,KAAK,CAAC,cAAc,EAAEA,KAAK,CAACX,QAAQ,CAACb,IAAI,CAAC;MACpD;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}